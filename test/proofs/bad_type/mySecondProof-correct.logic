|- ((P -> Q) & (!R -> !Q)) -> (P -> R) {
  {
		assume (P -> Q) & (!R -> !Q);
		{
			assume P;
			Reiteration [1] (P -> Q) & (!R -> !Q);
			OrE [3] P -> Q;
			ImpE [2, 4] Q;
			AndE [3] !R -> !Q;
			{
				assume !R;
				Reiteration [6] !R -> !Q;
				ImpE [7, 8] !Q;
				Reiteration [5] Q;
			}
			NegI [7-10] !!R;
			NnegE [11] R;
		}
		ImpI [2-12] P -> R;
	}
	ImpI [1-13] ((P -> Q) & (!R -> !Q)) -> (P -> R);
}