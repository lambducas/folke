|- ((P -> Q) & (!R -> !Q)) -> (P -> R) {
    {
		assume (P -> Q) & (!R -> !Q);
		{
			assume P;
			reiteration [1] (P -> Q) & (!R -> !Q);
			orE [3] P -> Q;
			impE [2, 4] Q;
			andE [3] !R -> !Q;
			{
				assume !R;
				reiteration [6] !R -> !Q;
				impE [7, 8] !Q;
				reiteration [5] Q;
			}
			negI [7-10] !!R;
			nnegE [11] R;
		}
		impI [2-12] P -> R;
	}
	impI [1-13] ((P -> Q) & (!R -> !Q)) -> (P -> R);
}