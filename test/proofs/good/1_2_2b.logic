!p|!q |- !(p&q){
    1: prem !p|!q;
    2: {
        3: assume !p;
        4: {
            5: assume p&q;
            6: AndEL[5] p;
            NotE[6, 3] bot; 
        }
        NotI[4] !(p&q);
    }
    3: {
        4: assume !q;
        5: {
            6: assume p&q;
            7: AndER[6] q;
            NotE[7, 4] bot; 
        }
        NotI[5] !(p&q);
    }
    OrE[1,2,3] !(p&q);
}