all x all y P(x,y) |- all u all v P(u,v) {
    1: prem all x all y P(x,y);
    2-6: {
        2: fresh x_0;
        3-5: {
            3: fresh y_0;
            4: AllE[1, x_0] all y P(x_0, y);
            5: AllE[4, y_0] P(x_0, y_0);
        }
        6: AllI[3-5] all v P(x_0, v);
    }
    7: AllI[2-6] all u all v P(u, v);
}