p|!q, r|!p, r|q |- r{
    1: prem p|!q;
    2: prem r|!p;
    3: prem r|q;
    4: {assume r;}
    5: {
        5: assume q;
        6: {
            6: assume p;
            7: {assume r;}
            8: {
                8: assume !p;
                9: NotE[6,8] bot;
                10: BotE[9] r;
            }
            11: OrE[2,7,8] r;
        }
        12: {
            12: assume !q;
            13: NotE[5,12] bot;
            14: BotE[13] r;
        }
        OrE[1,6, 12] r;
    }
    OrE[3,4, 5] r;
}