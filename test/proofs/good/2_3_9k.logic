all x (P(x) & Q(x)) |- all x P(x) & all x Q(x){
    1: prem all x (P(x) & Q(x));
    2-4: {
        2: fresh x_0;
        3: AllE[1, x_0] P(x_0) & Q(x_0);
        4: AndEL[3] P(x_0);
    }
    5: AllI[2-4] all x P(x);
    6-8: {
        6: fresh x_0;
        7: AllE[1, x_0] P(x_0) & Q(x_0);
        8: AndER[7] Q(x_0);
    }
    9: AllI[6-8] all x Q(x);
    10: AndI[5,9] all x P(x) & all x Q(x);
}