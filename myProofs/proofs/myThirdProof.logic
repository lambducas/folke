 |- (P -> Q) & (!R -> !Q) -> P -> R {
	1-14:{
		1:assume (P -> Q) & (!R -> !Q);
		2-13:{
			2:assume P;
			3:copy [1] (P -> Q) & (!R -> !Q);
			4:AndEL [3] P -> Q;
			5:IfE [2, 4] Q;
			6:AndER [3] !R -> !Q;
			7-11:{
				7:assume !R;
				8:copy [6] !R -> !Q;
				9:IfE [7, 8] !Q;
				10:copy [5] Q;
				11:NotE [10, 9] bot;
			}
			12:NotI [7-11] !!R;
			13:NotNotE [12] R;
		}
		14:IfI [2-13] P -> R;
	}
	15:IfI [1-14] (P -> Q) & (!R -> !Q) -> P -> R;
}